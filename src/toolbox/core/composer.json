{
    "name": "toolbox/core",
    "description": "Toolbox Core Package",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "require": {
        "php":">=5.3.3",
        "dayax/core": "@dev",
        "symfony/finder": "2.1.*",
        "symfony/filesystem": "2.1.*",
        "symfony/console": "2.1.*",
        "symfony/process": "2.1.*",        
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-invoker": "1.1.x"
    },
    "target-dir": "toolbox/core",
    "autoload":{
        "psr-0":{
            "toolbox\\core": ""
        }
    },
    "bin": [
        "resources/bin/phpunit",
        "resources/bin/toolbox",
        "resources/bin/composer.phar"
    ],
    "extra": {
        "branch-alias": "dev-master as 1.0.0-dev"
    }
}
